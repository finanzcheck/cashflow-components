{
  "name": "cashflow-components",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:finanzcheck/cashflow-components.git",
  "author": "Marcel Stieber <marcel.stieber@finanzcheck.de>",
  "license": "MIT",
  "scripts": {
    "build-token": "yarn theo tokens/json/example.json --transform web --setup tokens/setup.js --format custom-properties.css,module.js --dest tokens/out"
  },
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/typescript@3.1.39": {
          "rawConfig": {
            "tsconfig": {
              "compilerOptions": {
                "target": "ES5",
                "module": "CommonJS",
                "lib": [
                  "Es5",
                  "Es6",
                  "Es7",
                  "dom",
                  "ESNext"
                ]
              }
            }
          }
        }
      },
      "tester": "bit.envs/testers/jest@22.4.3"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "bit-bin": "14.7.6",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  },
  "devDependencies": {
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "prettier": "1.19.1",
    "theo": "8.1.5",
    "typescript": "3.8.3"
  }
}
